package workplace.service.first.colour;

public class PrimitiveDataTypeHomeWork {

	public static void main(String[] args) {
		
//Integer Data types Declaration
	
	byte lengthValue = 10;
	short widthValue = 25;
	int sizeValue = 40;
	long dimensionsValue = 20L;
	System.out.println("byte value is :");
	System.out.println("short value is :");
	System.out.println("int value is :");
	System.out.println("long value is :");

//Float Data type Declarations
	
	float breadthSize = 0.10f;
	double heightSize = 0.314d;
	
	System.out.println("float value is :");
	System.out.println("double value is :");
	
// Boolean data Type Declaration
	
	boolean b1 = true;
	
	System.out.println("Boolean value is :");
	
// Character data Type Declaration
	
	char totalDimensions, totalDimensions1;
	totalDimensions = 65;
	totalDimensions1 = 's';
	
	System.out.println("TotalDimensions value is :");
	System.out.println("TotalDimensions1 value is :");
	
	
// Non Primitive Data type Declaration
	
	String dimensions = "@ this will specify the dimensions 122"; 
	
	System.out.println("dimensions value is :");
	
}
}
