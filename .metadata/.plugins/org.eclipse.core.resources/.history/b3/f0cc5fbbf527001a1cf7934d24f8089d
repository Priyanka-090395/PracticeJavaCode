package workplace.service.second;

public class ConversionWrappers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			//Byte Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		
		System.out.println("Byte minimum value : " +Byte.MIN_VALUE);
		System.out.println("Byte maximum value : " +Byte.MAX_VALUE);
		System.out.println("Byte value : " +Byte.BYTES);
		System.out.println("Byte Size : " +Byte.SIZE);
		
		//Short Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		

		System.out.println("Short minimum value : " +Short.MIN_VALUE);
		System.out.println("Short maximum value : " +Short.MAX_VALUE);
		System.out.println("Short value : " +Short.BYTES);
		System.out.println("Short Size : " +Short.SIZE);
		
		
		//Integer Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		

		System.out.println("Integer minimum value : " +Integer.MIN_VALUE);
		System.out.println("Integer maximum value : " +Integer.MAX_VALUE);
		System.out.println("Integer value : " +Integer.BYTES);
		System.out.println("Integer Size : " +Integer.SIZE);
		
		//Long Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		

		System.out.println("Long minimum value : " +Long.MIN_VALUE);
		System.out.println("Long maximum value : " +Long.MAX_VALUE);
		System.out.println("Long value : " +Long.BYTES);
		System.out.println("Long Size : " +Long.SIZE);

		//Float Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		

		System.out.println("Float minimum value : " +Float.MIN_VALUE);
		System.out.println("Float maximum value : " +Float.MAX_VALUE);
		System.out.println("Float value : " +Float.BYTES);
		System.out.println("Float Size : " +Float.SIZE);
		
		// Double Class (MAX_VALUE, MIN_VALUE, BYTE, SIZE)
		

		System.out.println("Double minimum value : " +Double.MIN_VALUE);
		System.out.println("Double maximum value : " +Double.MAX_VALUE);
		System.out.println("Double value : " +Double.BYTES);
		System.out.println("Double Size : " +Double.SIZE);
		
		//Boolean Class (TRUE, FALSE)

		System.out.println("Boolean False : " +Boolean.FALSE);
		System.out.println("Boolean True : " +Boolean.TRUE);
	
		//Character Class 
		
		System.out.println("Character : "+Character.BYTES);
		System.out.println("Character :" +Character.MIN_VALUE);
		System.out.println("Character :" +Character.MAX_VALUE);
		
		
		
	}

}
