package workplace.service.second;

public class ConversionWrappers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

			//Byte Class (MAX_VALUE, MIN_VALUE,BYTE,SIZE)
		
		System.out.println("Byte minimum value : " +Byte.MIN_VALUE);
		System.out.println("Byte maximum value : " +Byte.MAX_VALUE);
		System.out.println("Byte value : " +Byte.BYTES);
		System.out.println("Byte Size : " +Byte.SIZE);
	}

}
