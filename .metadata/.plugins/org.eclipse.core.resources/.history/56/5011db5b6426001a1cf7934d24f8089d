package priyanka.java.project1;

public class StatementIfElse {

	int number;
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		StatementIfElse obj1 = new StatementIfElse();
		
		obj1.sumName (10,20);
		
		if (obj1.number == 30) {
			
			System.out.println("Print the statement as true");
				
			}
		
		else 
		{
			System.out.println("Print the statement as false");
			
		}
	}
	public int sumName(int add1, int add2) {
		
		number = add1 + add2;
		
		System.out.println("Sum of integers is: ");
		
		return number;
	}
	}

	
			
