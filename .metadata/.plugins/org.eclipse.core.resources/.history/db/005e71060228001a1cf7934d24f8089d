package home.work.project2;

public class TwoDimentionalArrayUsingFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[][] valueOne= new int[3][3]; //2-D array holding 3 int arrays(each array holding 3 int values).

		valueOne[0][0]=10; //inserting 10 in the first array at its 0th index 
		valueOne[0][1]=20; //inserting 20 in the first array at its 1st index 
		valueOne[0][2]=30; //inserting 30 in the first array at its 2nd index

		valueOne[1][0]=40; //inserting 40 in the second array at its 0th index 
		valueOne[1][1]=50; //inserting 50 in the second array at its 1st index
		valueOne[1][2]=60; //inserting 60 in the second array at its 2nd index

		valueOne[2][0]=70; //inserting 70 in the third array at its 0th index 
		valueOne[2][1]=80; //inserting 80 in the third array at its 1st index
		valueOne[2][2]=90; //inserting 90 in the third array at its 2nd index 



		//Reading an array
		System.out.println("Length of this array is : " + valueOne.length);
		System.out.println("Contents of this array are : ");

			for(int i=0; i<valueOne.length; i++)
			{
				for(int j=0; j<valueOne.length;j++)
				{
					System.out.println(valueOne[i][j]);
				}	
			}
		}

	}


